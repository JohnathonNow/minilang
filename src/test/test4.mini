var L := [1, 2, "hello", 4, 5]

for Key, Value in L do
	print('Key = {Key}, Value = {Value}\n')
end

var M := {"a" is 100, 10 is "b"}

for Key, Value in M do
	print('Key = {Key}, Value = {Value}\n')
end

fun test(X) do
	let (A, B, C) := X
	print('A = {A}, B = {B}, C = {C}\n')
end

test((1, 2, 3))
test([1, 2, 3])