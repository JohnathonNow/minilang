fun test1(N) do
	for I in 1 .. 10 do
		must print('I = {I}\n')
		for J in 1 .. 10 do
			must print('J = {I}\n')
			if (I * I) + (J * J) = (N * N) then
				exit exit '{I}^2 + {J}^2 = {N}^2'
			end
		end
	end
end

test1(5)

fun test2(F) do
	print('Starting\n')
	var I := 1
	must print('Must: I = {I}\n')
	I := 2
	I := F()
	print('Finishing: I = {I}\n')
	I := 3
end

do
	test2(;) 5
	test2(;) 1 / 0
on Error do
	print('{Error:type}: {Error:message}\n')
end
