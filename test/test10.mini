var PointT := object(:X, :Y)

var A := PointT(10, 20)

print('A:X = {A:X}\n')
print('A = {A}\n')

A:Y := 200

method(:string, PointT; P) 'point({P:X}, {P:Y})'

print('A = {A}\n')

method(::+, PointT, PointT; A, B) PointT(A:X + B:X, A:Y + B:Y)

var B := PointT(23, 9.1)

print('{A} + {B} = {A + B}\n')

method(::*, PointT, NumberT; A, N) PointT(A:X * N, A:Y * N)
method(::*, NumberT, PointT; N, A) PointT(N * A:X, N * A:Y)

print('{A} * 5 = {A * 5}\n')
print('7.1 * {B} = {7.1 * B}\n')