var Sum := 0
for I in 1 .. 5 do
	var Wait := I * 11 % 7
	print('{I}: waiting {Wait} seconds.\n')
	uv_sleep(1000 * Wait)
	print('{I}: done.\n')
	Sum := old + 1
end
print('Done: Sum = {Sum}\n')

var Sum := 0
parallel(1 .. 5; I) do
	var Wait := I * 11 % 7
	print('{I}: waiting {Wait} seconds.\n')
	uv_sleep(1000 * Wait)
	print('{I}: done.\n')
	Sum := old + 1
end
